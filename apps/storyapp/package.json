{
	"name": "@svelte-fui/storyapp",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"dev": "vite dev",
		"sync": "svelte-kit sync",
		"format": "prettier --plugin-search-dir . --write .",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"preview": "vite preview",
		"storybook:build": "svelte-kit sync && storybook build",
		"storybook:dev": "storybook dev -p 6006 --no-open"
	},
	"devDependencies": {
		"@storybook/addon-essentials": "^7.0.7",
		"@storybook/addon-interactions": "^7.0.7",
		"@storybook/addon-links": "^7.0.7",
		"@storybook/addon-mdx-gfm": "^7.0.7",
		"@storybook/addon-svelte-csf": "^3.0.2",
		"@storybook/blocks": "^7.0.7",
		"@storybook/manager-api": "7.0.7",
		"@storybook/svelte": "^7.0.7",
		"@storybook/sveltekit": "^7.0.7",
		"@storybook/testing-library": "^0.1.0",
		"@storybook/theming": "7.0.7",
		"@svelte-fui/core": "*",
		"@svelte-fui/tailwindcss": "*",
		"@svelte-fui/theme": "*",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.5.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.26.0",
		"postcss": "^8.4.23",
		"postcss-extend": "^1.0.5",
		"postcss-extend-rule": "^4.0.0",
		"postcss-nesting": "^11.2.2",
		"postcss-preset-env": "^8.3.2",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"storybook": "^7.0.7",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"tailwindcss": "^3.3.1",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.3.0"
	},
	"dependencies": {
		"@svelte-fui/icons": "^0.0.1"
	}
}
